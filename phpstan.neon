includes:
    - phpstan-baseline.neon

parameters:
    level: 2
    paths:
        - src
        - tests

    symfony:
        container_xml_path: %rootDir%/../../../var/cache/test/srcApp_KernelTestDebugContainer.xml

    bootstrapFiles:
        - vendor/bin/.phpunit/phpunit-8.5-0/vendor/autoload.php

    inferPrivatePropertyTypeFromConstructor: true
